{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
			"type": "string",
			"defaultValue": "[resourceGroup().location]",
			"metadata": {
				"description": "Location for all resources."
			}
		},
        "LAWConnectorName": {
			"type": "string",
			"defaultValue": "myLawConnector",
			"metadata": {
				"description": "Log Analytics Workspace ID."
			}
		},
        "LAWWorkspaceId": {
			"type": "string",
			"defaultValue": "10e9eb79-a0af-4487-9e91-074ef479fed2",
			"metadata": {
				"description": "Log Analytics Workspace ID."
			}
		},
        "LAWWorkspaceKey": {
			"type": "string",
			"defaultValue": "YmrSqX8W0PJCwXf7ISbLG5bGd0jTOmXEB2cSGekIPtqsSAADYdk7VaUkoWKOZeVcqkeTv4ofP8kCeQd2q9M0Tg==",
			"metadata": {
				"description": "Log Analytics Workspace Key."
			}
		}
    },
    "variables": {
    },
    "resources": [
        {
            "type": "Microsoft.Web/connections",
            "apiVersion": "2018-07-01-preview",
            "location": "[parameters('location')]",
            "name": "[parameters('LAWConnectorname')]",
            "dependsOn": [],
            "properties": {
                "displayName": "[parameters('LAWConnectorname')]",
                "customParameterValues": {},
                "api": {
                    "id": "[concat('/subscriptions/828ccdf7-38da-4aa4-b635-47ef8aec278e/providers/Microsoft.Web/locations/usgovvirginia/managedApis/azureloganalyticsdatacollector')]"
                },
                "parameterValues": {
                    "username": "[parameters('LAWWorkspaceId')]",
                    "password": "[parameters('LAWWorkspaceKey')]"
                }
            }
        }
    ]
}